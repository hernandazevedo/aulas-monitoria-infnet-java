<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib 
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
    version="2.0">
 
    <namespace>http://example.com/test</namespace>
 
    <tag>
        <tag-name>customComponent</tag-name>
		<component>
            <component-type>components.CustomComponent</component-type>
            <renderer-type>components.CustomComponent</renderer-type>
        </component>
        
		<attribute>
		    <name>id</name>
		    <type>java.lang.String</type>
		    <required>false</required>		    		   
		</attribute>
		<attribute>
		    <name>componenteId</name>
		    <type>java.lang.String</type>
		    <required>false</required>		    		   
		</attribute>
		<attribute>
		    <name>value</name>
		    <type>java.lang.String</type>
		    <required>false</required>		    		   
		</attribute>
		<attribute>
		    <name>style</name>
		    <type>java.lang.String</type>
		    <required>false</required>		    		   
		</attribute>
		<attribute>
		    <name>styleClass</name>
		    <type>java.lang.String</type>
		    <required>false</required>		    		   
		</attribute>
		<attribute>
		    <name>listenerAction</name>
		    <type>javax.el.MethodExpression</type>
		    <required>false</required>		    		   
		</attribute>
    </tag>
 
</facelet-taglib>